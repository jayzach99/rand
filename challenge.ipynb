   "source": [
    "import numpy as np\n",
    " \n",
    "# Solve\n",
    "def solve(x, y):\n",
    "    a = np.array([max(0, np.sign(x)), max(0,np.sign(y))]) * 2**np.array([0,1])\n",
    "    return np.sum(a)"
   ]
  },
